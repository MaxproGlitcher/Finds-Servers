		local tween = TweenService:Create(frame, TweenInfo.new(0.3), {
			BackgroundTransparency = 1,
			Position = UDim2.new(1, 300, 1, -20)
		})
		tween:Play()
		tween.Completed:Wait()
		frame:Destroy()
	end)
end

